{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "axe/text-alternatives": "off",
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 117",
    "not firefox <= 117",
    "not and_chr <= 117",
    "not chrome <= 117",
    "not edge <= 117",
    "not opera <= 102",
    "not samsung <= 22"
  ]
}